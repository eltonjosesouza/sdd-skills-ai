{
  "name": "sdd-skills-ai",
  "version": "1.0.14",
  "description": "CLI to scaffold a Spec-Driven Organization boilerplate",
  "main": "dist/index.js",
  "bin": {
    "sdd-skills-ai": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --clean",
    "dev": "tsup src/index.ts --format cjs --watch",
    "test": "jest --passWithNoTests",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "scaffold",
    "spec-driven",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eltonjosesouza/sdd-skills-ai"
  },
  "bugs": {
    "url": "https://github.com/eltonjosesouza/sdd-skills-ai/issues"
  },
  "homepage": "https://github.com/eltonjosesouza/sdd-skills-ai#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "config.json",
    "README.md",
    "AGENTS.md"
  ],
  "author": "Elton Jose de Souza",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
